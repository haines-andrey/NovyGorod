{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.log"
        }
      }
    ]
  },
  "DatabaseServer": "",
  "ConnectionStrings": {
    "MySQL": "server=;database=;user=;password=;",
    "MSSQL": "Server=;Database=;User Id=;Password=;"
  },
  "Url": "https://localhost:5001",
  "AllowedHosts": "*",
  "MediaDataDirectory": "media-data"
}
